{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"indent": [2, "tab"],
		"no-tabs": ["error", { "allowIndentationTabs": true }],
		"no-underscore-dangle": "off"
	}
}
